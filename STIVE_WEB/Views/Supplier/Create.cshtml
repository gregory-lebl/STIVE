@{
    ViewData["Title"] = "Test ajout fornisseur";
    @model STIVE_WEB.Models.Orders.Supplier;
}

    <div class="col-md-6">
        <form asp-controller="Supplier" asp-action="Create" method="post">
            <div class="mb-3">
                <label asp-for="Name" class="form-label">Nom du fournisseur</label>
                <input type="text" class="form-control" asp-for="Name" value="GregLeFournisseur">
            </div>
            <div class="mb-3">
                <label asp-for="Adress" class="form-label">Adresse</label>
                <input type="text" class="form-control" asp-for="Adress" value="123 rue aux chats">
            </div>
            <div class="mb-3">
                <label asp-for="Cp" class="form-label">Code Postal</label>
                <input type="text" class="form-control" asp-for="Cp" value="75000">
            </div>
            <div class="mb-3">
                <label asp-for="City" class="form-label">Ville</label>
                <input type="text" class="form-control" asp-for="City" value="Paris">
            </div>
            <div class="mb-3">
                <label asp-for="Siret" class="form-label">SIRET</label>
                <input type="text" class="form-control" asp-for="Siret" value="442 293 775 00031">
            </div>
            <div class="mb-3">
                <label asp-for="PhoneNumber" class="form-label">Téléphone</label>
                <input type="text" class="form-control" asp-for="PhoneNumber" value="02 35 46 53 76">
            </div>
            <button type="submit" class="btn btn-primary">Envoyer le formulaire</button>
        </form>
    </div>
